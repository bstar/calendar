{
  "name": "cla-calendar",
  "version": "0.1.0",
  "description": "A flexible date range picker component for React with drag selection support",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "rollup -c",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:coverage:serve": "npx http-server coverage -p 8080 -o",
    "prepare": "npm run build",
    "lint": "eslint --config eslint.config.js src/examples/CustomDateFormatterExample.tsx",
    "lint:all": "eslint --config eslint.config.js \"src/**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint --config eslint.config.js --fix \"src/**/*.{js,ts,tsx}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "lodash-es": "^4.17.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/runtime": "^7.16.0",
    "@eslint/js": "^9.22.0",
    "@mdx-js/react": "^3.1.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@storybook/addon-docs": "^9.0.16",
    "@storybook/addon-links": "^9.0.16",
    "@storybook/addon-onboarding": "^9.0.16",
    "@storybook/react-vite": "^9.0.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "cssnano": "^5.0.0",
    "eslint": "^9.22.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^9.0.16",
    "jest": "^29.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^2.60.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "storybook": "^9.0.16",
    "ts-jest": "^29.2.6",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vite": "^5.4.19",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "react",
    "calendar",
    "date-picker",
    "date-range",
    "drag-select"
  ],
  "author": "CLA",
  "license": "MIT"
